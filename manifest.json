{
  "manifest_version": 3,

  "name": "Double Middle-Click Close Tab",
  "description": "Double click mouse middle button to close current tab.",
  "version": "1.0.2",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "bg.js"
  },
  "action": {
    "default_icon": "icon.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["scripting", "storage"],
  "host_permissions": ["<all_urls>"]
}
